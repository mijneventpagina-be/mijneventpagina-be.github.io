    "@metalsmith/markdown": "^1.6.0",
    "@metalsmith/permalinks": "^2.4.0",
    "@metalsmith/sass": "^1.0.0",
    "browser-sync": "^2.27.11",
    "cheerio": "^1.0.0-rc.11",
